<?xml version="1.0" encoding="UTF-8"?>
<!--
GUI Label definition
-->
<gui>
    <properties>
        <one-value>
            <block>
                <class>book-user-unique-property-block</class>
            </block>
            <firstName>
                <label>Prénom</label>
                <type>text</type>
            </firstName>
            <lastName>
                <label>Nom de famille</label>
                <type>text</type>
            </lastName>
            <birthday>
                <label>Date de naissance</label>
                <type>date</type>
            </birthday>
        </one-value>
        <multi-values>
            <mail>
                <label>Email(s)</label>
                <add>Ajouter un email</add>
                <icon>glyphicon glyphicon-envelope</icon>
                <type>email</type>
                <input-class></input-class>
            </mail>
            <phone_number>
                <label>Numéro(s) de téléphone</label>
                <add>Ajouter un numéro de téléphone</add>
                <icon>glyphicon glyphicon-earphone</icon>
                <type>tel</type>
                <input-class></input-class>
            </phone_number>
            <adress>
                <label>Adresse(s)</label>
                <add>Ajouter une adresse</add>
                <icon>glyphicon glyphicon-map-marker</icon>
                <type>text</type>
                <input-class></input-class>
            </adress>
            <url>
                <label>Site(s)</label>
                <add>Ajouter un site</add>
                <icon>glyphicon glyphicon-globe</icon>
                <type>url</type>
                <input-class></input-class>
            </url>
            <note>
                <label>Note(s)</label>
                <add>Ajouter une note</add>
                <icon>glyphicon glyphicon-pencil</icon>
                <type>text</type>
                <input-class></input-class>
            </note>
            <other>
                <label>Autre(s)</label>
                <add>Ajouter une propriété autre</add>
                <type>text</type>
                <input-class></input-class>
            </other>            
        </multi-values>
        <add>
            <icon>glyphicon glyphicon-plus</icon>
        </add>
        <buttons>
            <cancel>
                <label></label>
                <icon>glyphicon glyphicon-trash</icon>
                <class>btn btn-danger</class>
            </cancel>
        </buttons>
    </properties>
    <communities>
        <label>Communauté(s)</label>
        <label-not-validated>Communauté(s) en attente de validation</label-not-validated>
        <class>btn btn-default</class>
    </communities>
</gui>
