<?xml version="1.0" encoding="UTF-8"?>
<!--
GUI Label definition
-->
<gui>
    <properties>
        <one-value>
            <firstName>
                <label>Prénom</label>
            </firstName>
            <lastName>
                <label>Nom de famille</label>
            </lastName>
            <birthday>
                <label>Date de naissance</label>
            </birthday>
        </one-value>
        <multi-values>
            <mail>
                <label>Email(s)</label>
                <add>Ajouter un email</add>
                <icon>icon-envelope</icon>
            </mail>
            <phone_number>
                <label>Numéro(s) de téléphone</label>
                <add>Ajouter un numéro de téléphone</add>
                <icon>icon-user</icon>
            </phone_number>
            <adress>
                <label>Adresse(s)</label>
                <add>Ajouter une adresse</add>
                <icon>icon-map-marker</icon>
            </adress>
            <url>
                <label>Site(s)</label>
                <add>Ajouter un site</add>
                <icon>icon-globe</icon>
            </url>
            <note>
                <label>Note(s)</label>
                <add>Ajouter une note</add>
                <icon>icon-pencil</icon>
            </note>
            <other>
                <label>Autre(s)</label>
                <add>Ajouter une propriété autre</add>
            </other>            
        </multi-values>
        <add>
            <icon>icon-plus</icon>
        </add>
        <buttons>
            <cancel>
                <label></label>
                <icon>icon-white icon-trash</icon>
                <class>btn btn-danger</class>
            </cancel>
        </buttons>
    </properties>
    <communities>
        <label>Communauté(s)</label>
    </communities>
</gui>
